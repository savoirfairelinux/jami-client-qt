<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Containers</key>
    <array>
        <!-- Migrate Application Support -->
        <dict>
            <key>Source</key>
            <string>~/Library/Application Support/com.yourcompany.appname</string>
        </dict>
        <!-- Migrate Caches -->
        <dict>
            <key>Source</key>
            <string>~/Library/Caches/com.yourcompany.appname</string>
        </dict>
    </array>
</dict>
</plist>
